<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!--Adding New Sequence For A Field-->
    <data noupdate="1">
        <!-- Sequences for hospital.patient -->
        <record id="seq_woocommerce_configuration" model="ir.sequence">
            <field name="name">WooCommerce Configuration Sequence</field>
            <field name="code">woocommerce.configuration.sequence</field>
            <field name="prefix">CON</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_woocommerce_orders" model="ir.sequence">
            <field name="name">WooCommerce Orders Sequence</field>
            <field name="code">woocommerce.orders.sequence</field>
            <field name="prefix">WO</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_woocommerce_products" model="ir.sequence">
            <field name="name">WooCommerce Products Sequence</field>
            <field name="code">woocommerce.products.sequence</field>
            <field name="prefix">WP</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_woocommerce_customers" model="ir.sequence">
            <field name="name">WooCommerce Customers Sequence</field>
            <field name="code">woocommerce.customers.sequence</field>
            <field name="prefix">WC</field>
            <field name="padding">3</field>
        </record>
        <!-- Sequences for hospital.appointment -->
        <!-- <record id="seq_hospital_appointment" model="ir.sequence">
